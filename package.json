  "description": "A repository to manage the releasing of AMPATH micro-frontends (es modules)",
  "main": "index.js",
  "scripts": {
    "build": "packmap -o openmrs/frontend --override-map ./override-import-map.json --exclude-packages ./exclude-packages.json",
